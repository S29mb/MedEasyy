{
  "expo": {
    "name": "Tektal_Pharma",
    "slug": "Tektal_Pharma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "medicineshebamaster",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "permissions": [
  "ACCESS_FINE_LOCATION"
],
    "experiments": {
      "typedRoutes": true
    }
  },

  "android": {
    "permissions": ["CAMERA"]
  },
  "ios": {
    "infoPlist": {
      "NSCameraUsageDescription": "Cette application utilise la cam√©ra pour scanner des ordonnances."
    }
  }
}
